    <![CDATA[SELECT PersonsEO.PERSON_ID, 
       PersonsEO.PRINCIPAL_NAME, 
       PersonsEO.TITLE, 
       PersonsEO.FIRST_NAME, 
       PersonsEO.LAST_NAME, 
       PersonsEO.PERSON_TYPE_CODE, 
       PersonsEO.SUPPLIER_ID, 
       PersonsEO.PROVISIONED_FLAG, 
       PersonsEO.PRIMARY_ADDRESS_ID, 
       PersonsEO.REGISTERED_DATE, 
       PersonsEO.MEMBERSHIP_ID, 
       PersonsEO.EMAIL, 
       PersonsEO.CONFIRMED_EMAIL, 
       PersonsEO.PHONE_NUMBER, 
       PersonsEO.MOBILE_PHONE_NUMBER, 
       PersonsEO.CREDIT_LIMIT, 
       PersonsEO.DATE_OF_BIRTH, 
       PersonsEO.MARITAL_STATUS_CODE, 
       PersonsEO.GENDER, 
       PersonsEO.CHILDREN_UNDER_18, 
       PersonsEO.APPROXIMATE_INCOME, 
       PersonsEO.CONTACT_METHOD_CODE, 
       PersonsEO.CONTACTABLE_FLAG, 
       PersonsEO.CONTACT_BY_AFFILLIATES_FLAG, 
       PersonsEO.CREATED_BY, 
       PersonsEO.CREATION_DATE, 
       PersonsEO.LAST_UPDATED_BY, 
       PersonsEO.LAST_UPDATE_DATE, 
       PersonsEO.OBJECT_VERSION_ID, 
       CustomerInterestsEO.CUSTOMER_ID, 
       CustomerInterestsEO.CATEGORY_ID, 
       CustomerInterestsEO.CUSTOMER_INTERESTS_ID, 
       AddressesEO.ADDRESS1, 
       AddressesEO.ADDRESS2, 
       AddressesEO.ADDRESS_ID, 
       AddressesEO.CITY, 
       AddressesEO.COUNTRY_ID, 
       AddressesEO.LATITUDE, 
       AddressesEO.LONGITUDE, 
       AddressesEO.OBJECT_VERSION_ID AS OBJECT_VERSION_ID1, 
       AddressesEO.POSTAL_CODE, 
       AddressesEO.STATE_PROVINCE
FROM PERSONS PersonsEO, CUSTOMER_INTERESTS CustomerInterestsEO, ADDRESSES AddressesEO, ADDRESS_USAGES AddressUsagesEO
WHERE (CustomerInterestsEO.CUSTOMER_ID(+)=PersonsEO.PERSON_ID) AND 
(PersonsEO.PERSON_ID = AddressUsagesEO.ASSOCIATED_OWNER_ID)AND
 (Addressusageseo.Address_Id = Addresseseo.Address_Id)And 
( AddressesEO.ADDRESS_ID=PersonsEO.PRIMARY_ADDRESS_ID)And
PersonsEO.PERSON_ID=108]]>
  </SQLQuery>
