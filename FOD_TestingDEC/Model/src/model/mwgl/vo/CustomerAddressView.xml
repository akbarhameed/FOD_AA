  <SQLQuery>
    <![CDATA[SELECT AddressEO.ADDRESS_ID, 
       AddressEO.ADDRESS1, 
       AddressEO.ADDRESS2, 
       AddressEO.CITY, 
       AddressEO.POSTAL_CODE, 
       AddressEO.STATE_PROVINCE, 
       AddressEO.COUNTRY_ID, 
       AddressEO.LONGITUDE, 
       AddressEO.LATITUDE, 
       AddressEO.OBJECT_VERSION_ID, 
       AddressUsageEO.ADDRESS_USAGES_ID, 
       AddressUsageEO.ASSOCIATED_OWNER_ID, 
       AddressUsageEO.OWNER_TYPE_CODE, 
       AddressUsageEO.ADDRESS_ID AS ADDRESS_ID1, 
       AddressUsageEO.USAGE_TYPE_CODE, 
       AddressUsageEO.EXPIRED_FLAG, 
       AddressUsageEO.OBJECT_VERSION_ID AS OBJECT_VERSION_ID1, 
       CountryCodeEO.ISO_COUNTRY_CODE, 
       CountryCodeEO.COUNTRY_NAME, 
       CountryCodeEO.LANGUAGE, 
       LookupCodeEO.MEANING, 
       CountryCodeEO.OBJECT_VERSION_ID AS OBJECT_VERSION_ID2, 
       LookupCodeEO.LOOKUP_TYPE, 
       LookupCodeEO.LOOKUP_CODE, 
       LookupCodeEO.LANGUAGE AS LANGUAGE1
FROM ADDRESSES AddressEO, ADDRESS_USAGES AddressUsageEO, COUNTRY_CODES CountryCodeEO, LOOKUP_CODES LookupCodeEO
WHERE (AddressEO.ADDRESS_ID = AddressUsageEO.ADDRESS_ID) AND (AddressEO.COUNTRY_ID = CountryCodeEO.ISO_COUNTRY_CODE) AND CountryCodeEO.LANGUAGE = 'EN' AND LookupCodeEO.LOOKUP_TYPE='USAGE_TYPE_CODE' and LookupCodeEO.LOOKUP_CODE = AddressUsageEO.USAGE_TYPE_CODE AND LookupCodeEO.LANGUAGE = 'EN'
ORDER BY AddressEO.ADDRESS1,AddressEO.CITY]]>
  </SQLQuery>
